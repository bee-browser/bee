#[derive(Clone, Copy)]
pub enum HtmlTag {
    {{#each tags}}
    {{{this}}},
    {{/each}}
}

impl HtmlTag {
    pub fn name(&self) -> &'static str {
        DATA[self.index()].name
    }

    pub fn deprecated(&self) -> bool {
        DATA[self.index()].deprecated
    }

    fn index(&self) -> usize {
        (*self) as usize
    }
}

const DATA: [HtmlTagData; {{{num_tags}}}] = [
    {{#each data}}
    HtmlTagData { name: "{{{name}}}", deprecated: {{{deprecated}}} },
    {{/each}}
];

const TAGS: phf::Map<&'static str, HtmlTag> = {{{phf}}};
