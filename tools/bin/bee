#!/usr/bin/env node

'use strict';

const program = require('commander');

program
  .description(
    'Tools for developers')
  .command(
    'lms', 'Layout message source')
  .command(
    'codegen', 'Generate code using a template with variables')
  .command(
    'webui', 'Web UI for testing')
  .parse(process.argv);
